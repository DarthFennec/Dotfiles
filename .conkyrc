background no
out_to_x no
out_to_console yes
update_interval 1.0
format_human_readable no

TEXT
:   ${cpu cpu1}       :   ${cpu cpu2} \
:   ${cpu cpu3}       :   ${cpu cpu4} \
:   $swapperc         :   $memperc    \
:   !                 :   !           \
:   ${texeci 10 nvidia-settings -tq GPUCoreTemp} \
:   ${hwmon 1 temp 5} \
:   ${hwmon 0 temp 1} :   ${hwmon 0 temp 2} \
:   ${hwmon 1 temp 1} :   ${hwmon 1 temp 2} \
:   ${hwmon 1 temp 3} :   ${hwmon 1 temp 4} \
: * ${upspeed eth0}   : * ${downspeed eth0} \
:   !                 :   !
