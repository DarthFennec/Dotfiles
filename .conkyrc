conky.config={
  background=false,
  out_to_x=false,
  out_to_console=true,
  update_interval=1.0,
  format_human_readable=false
}

conky.text=[[
:   ${cpu cpu1}       :   ${cpu cpu2} \
:   ${cpu cpu3}       :   ${cpu cpu4} \
:   ${cpu cpu5}       :   ${cpu cpu6} \
:   ${cpu cpu7}       :   ${cpu cpu8} \
:   $swapperc         :   $memperc    \
:   !                 :   !           \
: % 4                                       \
  % ${hwmon 0 temp 1}   % ${hwmon 1 temp 1} \
  % ${hwmon 2 temp 1}   % ${hwmon 2 temp 2} \
  % ${hwmon 2 temp 3}   % ${hwmon 2 temp 4} \
  % ${hwmon 2 temp 5}   % ${hwmon 2 temp 6} \
  % ${hwmon 2 temp 7}   % ${hwmon 2 temp 8} \
  % ${hwmon 2 temp 9}                       \
: * ${upspeed eno2}   : * ${downspeed eno2} \
:   !                 :   !
]]
